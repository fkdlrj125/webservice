class Super{
	private int numOne; 
	int numTwo;
	
	void showOne() {
		System.out.println("showOne() 실행됨");
	}
	public void showTwo() {
		System.out.println("showTwo() 실행됨");
	}
}

class Sub extends Super{
	void showOne() {
		System.out.println("===============================");
		System.out.println("오버라이딩된 showOne() 실행됨");
		System.out.println("===============================");
	}	
}

public class AccessEx02 {
	public static void main(String[] args) {
		Sub s = new Sub();
		s.showOne();
		s.showTwo();
	}
}
