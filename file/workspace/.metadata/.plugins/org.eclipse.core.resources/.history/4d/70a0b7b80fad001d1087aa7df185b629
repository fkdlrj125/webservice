class Buyer{
	int money = 1000;
	int bonusPoint = 0;
	
	void buy(Product P) {
		if(money < p.price) {
			System.out.println("");
			return;
		}
		money -= p.price;
		bonusPoint += p.bonusPoint;
		System.out.println(p+"을/를 구입하셨습니다.");
	}
}
class Product{
	
}

public class PolymorphismEx03 {
	public static void main(String[] args) {
		
	}
}
